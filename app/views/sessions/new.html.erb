<div class="login-page">
  <div class="form">
    <h1>Login</h1>
    <%=form_tag sessions_path, class:'login-form' do %>
    <%=text_field_tag :email, nil, placeholder:'email' %>
    <%=password_field_tag :password, nil, placeholder: 'password' %> </p>
    <%=button_tag 'login' %>
    <p class="message" id="button_clicked">Not registered? <%= link_to 'Create an Account', new_shop_owner_path %></p>
    <% end %>
  </div>
</div>

